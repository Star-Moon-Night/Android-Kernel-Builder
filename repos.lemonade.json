[
  {
"kernelSource":{
"name":"StarMoonNight-KernelSU-pre",
"repo":"https://github.com/Xtended-Devices/kernel_oneplus_sm8350.git",
"branch":"xt",
"device":"lemonade",
"defconfig":"vendor/lahaina-qgki_defconfig"
},
"withKernelSU":true,
